<!-- Edit visualizaion form-->
<h1 class="page-title">Visualize - Editing Mode</h1>

<div class="navigation-options">
    <%= link_to '< Back', @visualization %> | 
    <%= link_to 'Delete', delete_visualization_visualizations_path(:id => @visualization.id), data: {confirm: "Are you sure you want to delete?"} %>
</div>

<div class="viz-page">
    <%= form_with(model: @visualization, local: true, url: visualization_path(@visualization), method: "patch") do |form_builder|
            render 'form', locals: { fb: form_builder }
        end %>
</div>
